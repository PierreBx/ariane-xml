				<xs:annotation>
					<xs:documentation xml:lang="fr">
						<doc:name>Identifiant DSN de la déclaration annulée et remplacée</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<!-- obligatoire pour les traitements -->
			<xs:element name="S20_G00_96_902" type="n4ds_dt:Nombre_1_6_Suivi" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">
						<doc:name>Identifiant de la DSN dans le flux</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
