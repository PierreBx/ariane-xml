					<xs:documentation xml:lang="fr">
						<doc:name>Adresse téléphonique</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="S20_G00_07_003" type="n4ds_dt:N4DS_Email_6-100" minOccurs="1" maxOccurs="1">
