					<xs:documentation xml:lang="fr">
						<doc:name>Raison sociale du déclarant</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="S10_G00_95_900" type="n4ds_dt:Type_Alphanumeric_1-50" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">
						<doc:name>Identifiant du flux</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>		
			<xs:element name="S10_G00_95_901" type="n4ds_dt:Type_Alphanumeric_1-100" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation xml:lang="fr">
						<doc:name>Adresse mél du déclarant</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

	<!--Description bloc3_DSN_vers_PoleEmploi_S20_G00_07_Type -->
	<xs:complexType name="bloc3_DSN_vers_PoleEmploi_S20_G00_07_Type">
