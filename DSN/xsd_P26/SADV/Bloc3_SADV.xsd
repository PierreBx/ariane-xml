				<xs:annotation>
					<xs:documentation xml:lang="fr">
						<doc:name>Nom et prénom du contact</doc:name>
					</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="S20_G00_07_002" type="n4ds_dt:Telephone" minOccurs="1" maxOccurs="1">
